{
  "name": "@destinationstransfers/eslint-plugin",
  "version": "1.0.0",
  "description": "Shareable ESLint config/plugin for our repos",
  "main": "index.js",
  "engines": {
    "node": ">=8.2"
  },
  "peerDependencies": {
    "eslint": "^5.13.0"
  },
  "devDependencies": {
    "eslint": "5.15.2",
    "mocha": "6.0.2"
  },
  "scripts": {
    "test:itself": "eslint *.js --parser-options=sourceType:script --env=node -c config.js",
    "test:prettier": "eslint --print-config . | eslint-config-prettier-check",
    "test": "npm run test:itself && npm run test:prettier && mocha local-rules/*.test.js",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";"
  },
  "files": [
    "index.js",
    "rules",
    "config.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destinationstransfers/eslint-plugin.git"
  },
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/destinationstransfers/eslint-plugin/issues"
  },
  "homepage": "https://github.com/destinationstransfers/eslint-plugin#readme",
  "dependencies": {
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsdoc": "^4.4.2",
    "eslint-plugin-lit": "^0.6.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-optimize-regex": "latest",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-sonarjs": "^0.3.0",
    "eslint-plugin-sort-requires": "latest",
    "eslint-plugin-unicorn": "^7.1.0",
    "is-html": "^1.1.0",
    "prettier": "^1.16.4"
  },
  "eslintConfig": {
    "extends": [
      "./config.js"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  }
}
