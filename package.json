{
  "name": "@destinationstransfers/eslint-config",
  "version": "4.0.0",
  "description": "Shareable ESLint config for our repos",
  "main": "index.js",
  "engines": {
    "node": ">=8.2"
  },
  "peerDependencies": {
    "eslint": "^5.10.0"
  },
  "devDependencies": {
    "eslint": "5.12.1"
  },
  "scripts": {
    "test:itself": "eslint *.js --parser-options=sourceType:script --env=node -c index.js",
    "test:prettier": "eslint --print-config . | eslint-config-prettier-check",
    "test": "npm run test:itself && npm run test:prettier"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destinationstransfers/eslint-config.git"
  },
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/destinationstransfers/eslint-config/issues"
  },
  "homepage": "https://github.com/destinationstransfers/eslint-config#readme",
  "dependencies": {
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-jsdoc": "^4.1.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-optimize-regex": "latest",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-sonarjs": "^0.2.0",
    "eslint-plugin-sort-requires": "latest",
    "prettier": "^1.16.2"
  },
  "eslintConfig": {
    "extends": [
      "./index.js"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  }
}
